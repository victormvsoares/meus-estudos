#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main(){

    setlocale(LC_ALL,"Portuguese");

    int n;

    printf("\t\tsequencia de fibonacci");
    
    printf("\n\tDigite um numero = ");
    scanf("%i",&n);

    printf("\to %iยบ termo da sequencia de fibonacci e %i",n,f(n));

    return 0;
}

int f(int n){

    if(n==1){

        return 1;

    }
    if(n==2){

        return 1;

    }
    else{

        return f(n-1) + f(n-2);

    }

}
