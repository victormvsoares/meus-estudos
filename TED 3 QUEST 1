#include <stdio.h>
#include <stdlib.h>

int main(){

    int numInt1;
    int numInt2;

    printf("Digite um numero inteiro : ");
    scanf("%i",&numInt1);

    printf("Digite um outro numero inteiro : ");
    scanf("%i",&numInt2);

    if(numInt1 > numInt2){

        printf("O numero maior e -> %i",numInt1);

    }

    else if(numInt1 < numInt2){

        printf("O numero maior e -> %i",numInt2);

    }
    
    else{

        printf("Os dois numeros sao iguais");

    }

    return 0;
}   
